{
    "compilerOptions": {
        "module": "commonjs",
        "declaration": true,
        "jsx": "preserve",
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "esModuleInterop": true,
        "target": "es6",
        "noEmit": true,
        "sourceMap": true,
        "lib": ["dom", "dom.iterable", "esnext"]
    },
    "exclude": ["node_modules"]
}
